<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tecxpert.myeduapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyEduApplication">
        <activity
            android:name=".activites.BookActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".activites.VedioActivity"
            android:exported="true"
            android:screenOrientation="landscape"

            />
        <activity
            android:name=".activites.StudyActivity"
            android:exported="true"
            android:label="@string/title_activity_study"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyEduApplication.NoActionBar" />
        <activity
            android:name=".activites.MainScreenActivity"
            android:exported="true"
            android:label="@string/title_activity_main_screen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyEduApplication.NoActionBar" />
        <activity
            android:name=".activites.LoginActivity"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".activites.SignupActivity"
            android:screenOrientation="portrait"
            android:exported="true" />
        <activity
            android:name=".activites.MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>